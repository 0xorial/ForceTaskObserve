<?xml version="1.0" encoding="utf-8"?>
<package>
    <metadata>
        <id>TS.{{AssemblyProduct}}</id>
        <version>{{AssemblyFileVersion}}</version>
        <title>{{AssemblyTitle}}</title>
        <authors>{{AssemblyCompany}}</authors>
        <owners>{{AssemblyCompany}}</owners>
        <projectUrl>https://github.com/0xorial/ForceTaskObserve</projectUrl>
        <licenseUrl>https://opensource.org/licenses/MIT</licenseUrl>
        <iconUrl>https://raw.githubusercontent.com/0xorial/ForceTaskObserve/master/eye-scanner.png</iconUrl>
        <summary>{{AssemblyDescription}}</summary>
        <description>
            Sync code invoking async code is a potential source of bugs. This R# plugin helps to find such places easily.
        </description>
        <copyright>{{AssemblyCopyright}}</copyright>
        <tags>async await task exception</tags>
        <dependencies>
            <dependency id="Wave" version="8.0" />
        </dependencies>
    </metadata>
    <files>
      <file src="" target="DotFiles" />
    </files>
</package>